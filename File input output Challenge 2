print("\nopening the file")

notepad = open("names.txt", "w")
name1 = "Bob\n"
name2 = "John\n"
name3 = "Josh\n"
name4 = "Rehan\n"
name5 = "Ronaldo\n"

notepad.write(name1)
notepad.write(name2)
notepad.write(name3)
notepad.write(name4)
notepad.write(name5)
notepad.close()

print("\nGo and read the file")
